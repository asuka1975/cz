// DESCRIPTION: 論理演算子と短絡評価が正しくコード生成されることを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT:

fn main() -> i32 {
    // AND
    let a: bool = true && true;    // true
    let b: bool = true && false;   // false
    let c: bool = false && true;   // false (短絡評価)

    // OR
    let d: bool = false || true;   // true
    let e: bool = true || false;   // true (短絡評価)
    let f: bool = false || false;  // false

    // NOT
    let g: bool = !true;   // false
    let h: bool = !false;  // true

    0
}
