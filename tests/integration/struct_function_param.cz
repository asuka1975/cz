struct Point { x: i32, y: i32 }

fn distance_sq(p: Point) -> i32 {
    p.x * p.x + p.y * p.y
}

fn main() -> i32 {
    let p = Point { x: 3, y: 4 };
    print_i32(distance_sq(p));
    0
}
