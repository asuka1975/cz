// TEST: ラベル付きcontinue
// EXPECT: success
// DESCRIPTION: ラベル付きcontinueで外側ループの次のイテレーションへスキップすることを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT: 3\n

fn main() -> i32 {
    let mut count = 0;
    let mut i = 0;
    'outer: while i < 3 {
        i = i + 1;
        let mut j = 0;
        while j < 3 {
            j = j + 1;
            if j == 2 {
                count = count + 1;
                continue 'outer;
            }
        }
    }
    print_i32(count);
    0
}
