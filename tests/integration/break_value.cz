// TEST: breakで値を返すwhileループ
// EXPECT: success
// DESCRIPTION: while式でbreakにより値を返し、その値を使用できることを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT: 5\n

fn main() -> i32 {
    let mut i = 0;
    let result = while i < 100 {
        if i == 5 {
            break i;
        }
        i = i + 1;
    };
    print_i32(result);
    0
}
