// TEST: ループラベルによる外側ループ制御
// EXPECT: success
// DESCRIPTION: ラベル付きbreakで外側のwhileループを抜ける動作が正しいことを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT: 4\n2\n

fn main() -> i32 {
    let mut i = 0;
    let mut found_j = 0;
    'outer: while i < 10 {
        let mut j = 0;
        while j < 10 {
            if i * 10 + j == 42 {
                found_j = j;
                break 'outer;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    print_i32(i);
    print_i32(found_j);
    0
}
