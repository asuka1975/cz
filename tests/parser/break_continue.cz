// TEST: break/continue文
// EXPECT: success
// DESCRIPTION: break（値付き・ラベル付き）とcontinueのパースが正しく行われることを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT: 5\n

fn main() -> i32 {
    let mut i = 0;
    let result = while i < 10 {
        if i == 5 {
            break i;
        }
        i = i + 1;
        continue;
    };
    print_i32(result);
    0
}
