// TEST: 演算子の優先順位
// EXPECT: success
// DESCRIPTION: 演算子の優先順位が正しく解析されることを検証する
// EXPECTED_EXIT_CODE: 0
// EXPECTED_STDOUT:

fn main() -> i32 {
    // 乗算が加算より優先
    let a: i32 = 2 + 3 * 4;  // 14

    // 括弧で優先順位を変更
    let b: i32 = (2 + 3) * 4;  // 20

    // 単項マイナス
    let c: i32 = -5 + 10;  // 5

    // 論理演算子の優先順位
    // && は || より優先
    let d: bool = true || false && false;  // true

    // 比較演算と論理演算の組み合わせ
    let e: i32 = if 1 < 2 && 3 > 0 { 1 } else { 0 };  // 1

    0
}
